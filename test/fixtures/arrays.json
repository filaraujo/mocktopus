{
  "arrayEmpty": {
    "@length": "0"
  },

  "arraySingleEmpty": {
    "@length": "1"
  },

  "arrayManyEmpty": {
    "@length": 10
  },

  "arraySingleObject": {
    "@length": 1,
    "someProperty": true
  },

  "arrayManyObjects": {
    "@length": 3,
    "someProperty": true
  },

  "arraySingleValue": {
    "@length": "1",
    "@value": "111"
  },

  "arrayManyValues": {
    "@length": "4",
    "@value": "444"
  },

  "arrayWithRange": {
    "@length": "1...5"
  },

  "arrayWithNestArray": {
    "@length": "1...2",
    "foo": {
      "@length": "1...3",
      "bar" : true
    }
  }
}
